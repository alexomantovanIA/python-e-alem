create table SIMULACOES(
    id NUMBER GENERATED ALWAYS AS IDENTITY PRIMARY KEY,
    ATIVIDADE VARCHAR2(50 BYTE),
    CUSTO_ANUAL FLOAT,
    TIPO_COBERTURA VARCHAR2(50 BYTE),
    PREMIO_ANUAL FLOAT,
    PREMIO_MENSAL FLOAT,
    MES NUMBER (38,0),
    REGIAO VARCHAR2(50 BYTE),
    VALOR_COBERTURA FLOAT);
